<form class="form-inline" role="form" ng-controller="FFClub">
  <div class="form-group">
    <div class="input-group">
      <div class="input-group-addon">輸入社團名稱：</div>
        <input type="text" ng-model="FFClub" placeholder="請輸入..." typeahead="club as club.Name for club in clubs | filter:{Name:$viewValue} | limitTo:10" typeahead-template-url="club.html" class="form-control" name="GroupName" autofocus>
    </div>
    <button type="submit" class="btn btn-primary">新增</button>
  </div>


<script type="text/ng-template" id="club.html">
      <li class="list">
        <div class="list-group-item">{{match.model.Name}}({{match.model.Location}})</div>
      </li>
</script>